# Environment Variables for Printify POD Manager
# Copy this file to .env and fill in your API keys

# fal.ai API key for image generation (recommended)
FAL_KEY=your_fal_api_key_here

# Alternative image generation services (fallback options)
OPENAI_API_KEY=your_openai_key_here
STABILITY_API_KEY=your_stability_key_here

# Neon Database Connection (from console.neon.tech)
DATABASE_URL=postgresql://username:password@host:port/database

# Shopify App Integration (server-side)
# Obtain these from your Shopify Partner app settings. Do NOT commit real secrets.
SHOPIFY_API_KEY=your_shopify_api_key_here
SHOPIFY_API_SECRET=your_shopify_api_secret_here
SHOPIFY_APP_URL=https://your-app.example.com
SCOPES=write_products,read_products,write_files

# Secure Printify -> Shopify sync endpoint token (shared secret)
PRINTIFY_SYNC_TOKEN=replace_with_strong_random_string
